    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "/Users/aero/Documents/Medical-Hero/home.html" ,
            "webRoot": "${workspaceFolder}"
        },
        
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "pwa-chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "name": "Launch Extension",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm",
            "request": "launch",
            "type": "pwa-extensionHost"
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Open home.html",
            "file": "/Users/aero/Documents/Medical-Hero/home.html"
        }
    ]
}